p = int(input("Enter p: "))
q = int(input("Enter q: "))
n = p*q
print("n: ", n)
phi = (p-1)*(q-1)
print("phi: ",phi)
e = int(input("Enter e: "))
d=int(pow(e,-1,phi))
print("d: ", d)
print("Public key: ",e, n)
print("Private key:" ,d, n)
msg = int(input("Enter message: "))
print("Original message:",msg)
C = pow(msg, e,n)
print("Encrypted message: ",C)
M = pow(C, d,n)
print("decrypted message: ",M)

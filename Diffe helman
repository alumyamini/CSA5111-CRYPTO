q=int(input("enter the prime number:"))
x=int(input("primitive root:"))
a=int(input("private key a:"))
b=int(input("private key b:"))
s = int(pow(x,a,q))
t = int(pow(x,b,q)) 
ka = int(pow(t,a,q))
kb = int(pow(s,b,q))
print('Secret key for the ebinezare is : ',ka)
print('Secret Key for the yeshuva is : ',kb)
